# Environment Variables
# Add your API keys here after importing to Glitch

# OpenSea API key for NFT data
OPENSEA_API_KEY=

# OpenAI GPT API key for image analysis and card generation
GPT_API_KEY=

# GoAPI API key for image generation
GOAPI_API_KEY=

# Server port (Glitch will override this)
PORT=3000

# Set to production for optimized settings
NODE_ENV=production